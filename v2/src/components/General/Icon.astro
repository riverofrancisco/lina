---
export interface Props {
  icon: string;
  color?: string;
}


const { icon  } = Astro.props as Props;
//const svgPath = `/media/icons/${icon}.svg`;
const { default: innerHTML } = await import(`../../../public/media/icons/${icon}.svg?raw`);
export const prerender = false;
---

<Fragment set:html={innerHTML} style={{ color: 'currentColor' }} />

