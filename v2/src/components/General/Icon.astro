---
export interface Props {
  icon: string;
}


const { icon } = Astro.props as Props;
const svgPath = `/media/icons/${icon}.svg`;
//const { default: innerHTML } = await import( /* @vite-ignore */ `../../utils/icons/${icon}.svg?raw`);
export const prerender = true;
---

<img src={svgPath} alt={icon}/>
